                 </exclusion>
             </exclusions>-->
        </dependency>

        <!-- @Slf4j-->
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>

        <!-- swagger 接口规范-->

        <dependency>
            <groupId>io.springfox</groupId>
